    "@toruslabs/session-manager": "^1.0.0",
    "@toruslabs/starkware-crypto": "^3.1.0",
    "@toruslabs/tweetnacl-js": "^1.0.4",
    "base64url": "^3.0.1",
    "bip39": "^3.1.0",
    "bn.js": "^5.2.1",
    "bowser": "^2.11.0",
